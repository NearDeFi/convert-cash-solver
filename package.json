{
    "type": "module",
    "name": "docker",
    "packageManager": "yarn@4.9.1",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "build:proxy": "cd ./contracts/proxy && RUSTFLAGS='-C link-arg=--enable-bulk-memory' cargo near build non-reproducible-wasm",
        "build:sandbox": "cd ./contracts/sandbox && RUSTFLAGS='-C link-arg=--enable-bulk-memory' cargo near build non-reproducible-wasm",
        "phala:test": "phala cvms create --name shade-agent-api-test --compose ./docker-compose.yaml --env-file ./.env.development.local",
        "docker:test": "sudo docker build -t myapp . && sudo docker run -p 3000:3000 --env-file .env.development.local --rm -e PORT=3000 myapp",
        "docker:prune": "sudo docker system prune",
        "docker:restart": "sudo systemctl restart docker",
        "docker:restart:mac": "osascript -e 'quit app \"Docker\"' && open -a Docker",
        "docker:image": "sudo docker build --no-cache -t mattdlockyer/shade-agent-api-test:latest .",
        "docker:push": "sudo docker push mattdlockyer/shade-agent-api-test",
        "start": "node --loader ts-node/esm ./src/app.js",
        "dev": "nodemon --exec \"node --loader ts-node/esm\" ./src/app.js",
        "dev:ts": "nodemon --exec \"node --loader ts-node/esm\" ./src/app.ts",
        "cli": "node --loader ts-node/esm ./cli.js",
        "cli:proxy": "yarn build:proxy && node --loader ts-node/esm ./cli.js ./contracts/proxy/target/near/contract.wasm",
        "type-check": "tsc --noEmit",
        "test": "node --loader ts-node/esm ./src/bus/main.ts",
        "bus": "node --loader ts-node/esm ./src/bus/main.ts",
        "bus:dev": "nodemon --exec \"node --loader ts-node/esm\" ./src/bus/main.ts",
        "user-quote": "node --loader ts-node/esm ./src/bus/quoteGenerator.ts"
    },
    "dependencies": {
        "@hono/node-server": "^1.14.2",
        "@near-js/accounts": "^2.2.2",
        "@near-js/crypto": "^2.2.2",
        "@near-js/providers": "^2.2.2",
        "@near-js/signers": "^2.2.2",
        "@near-js/tokens": "^2.2.2",
        "@near-js/utils": "^2.2.2",
        "@neardefi/shade-agent-js": "^1.0.4",
        "@types/elliptic": "^6",
        "@types/node": "^22.15.21",
        "borsh": "^2.0.0",
        "bs58": "^6.0.0",
        "dotenv": "^16.5.0",
        "ethers": "^6.15.0",
        "hono": "^4.7.10",
        "keccak": "^3.0.4",
        "near-seed-phrase": "^0.2.1",
        "node-fetch": "^3.3.2",
        "tronweb": "^6.0.3",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@types/bs58": "^5.0.0",
        "@types/ws": "^8.18.1",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}
